  "peerDependencies": {
    "@storybook/addons": ">=6.1.0",
    "@storybook/react": ">=5.2.0",
    "styled-components": ">=5.2.0"
  },
  "keywords": [
    "storybook",
    "styled-component",
    "addon",
    "theme",
